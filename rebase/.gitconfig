[alias]
  pr      = pull --rebase
  rb      = rebase
  rba     = rebase --abort
  rbc     = rebase --continue
  rbm     = rebase master
  rbs     = rebase --skip
  rc      = rebase --continue
  ri      = rebase -i
  rim     = rebase -i master
  arc     = add-rebase-continue
  oc      = original-commit
  oh      = orig-head
  shoc    = !git show `git original-commit`
  shoh    = !git show `git orig-head`
