[alias]
  dtc     = diff-theirs-conflicting
  dth     = diff-theirs
  dthc    = diff-theirs-conflicting
  pr      = pull --rebase
  rb      = rebase
  rba     = rebase --abort
  rbc     = rebase --continue
  rbh     = rebase-head
  rbp     = rebase-parent
  rbo     = rebase --onto
  rbm     = rebase master
  rbs     = rebase --skip
  rc      = rebase --continue
  ri      = rebase -i
  rim     = rebase -i master
  rq      = rebase-sequence
  arc     = add-rebase-continue
  oc      = original-commit
  oh      = original-head
  shoc    = show-original-commit
  shoh    = show-original-head
