[alias]
        cur     = log -1 --oneline
        files   = diff-tree --no-commit-id --name-only -r
        history = log --full-diff -p
        L       = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%n%n%B' --date=relative
        lsc     = diff-tree --no-commit-id --name-only -r
